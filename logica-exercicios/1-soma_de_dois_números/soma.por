// Crie um algoritmo que leia dois números inteiros informados pelo usuário e exiba a soma entre eles.

programa {
  funcao inicio() {
    inteiro a, b
    escreva("Escreva o valor do número A: ")
    leia(a)
    escreva("Escreva o valor do número B: ")
    leia(b)
    escreva ("Resultado: ", a + b )
  }
}
